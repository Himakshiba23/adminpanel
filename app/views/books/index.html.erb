  <div id="setFormCenter">
    <div class="col-sm-10 form-group-lg">
    <p id="notice"><%= notice %></p>
    
    <h1>Books</h1>
    <div id="paginator">
      <%= paginate @books, remote: true %>
    </div>
    <br>

    <table class="table table-hover table-bordered table-success" id="books">
      <thead>
        <tr>
          <th>Name</th>
          <th colspan="3"></th>
        </tr>
      </thead>
    
      <tbody>
          <%= render partial: 'book', collection: @books %>
      </tbody>
    </table>
    
    <br>
    <%= link_to "Export CSV", books_path(format: "csv"), style:"float: right" %>
    <%= link_to 'New Book', new_book_path ,{:remote => true, 'data-toggle' =>  "modal", 'data-target' => '#modal-window'}  %>
    
    <%= form_tag import_books_path, multipart: true do %>
      <%= file_field_tag :file %> 
      <%= submit_tag "Import CSV" %>
    <% end %>
  </div>
</div>  

<div id="modal-window-show" class="modal hide fade" role="dialog" aria-labelledby="myModalLabelShow" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content"></div>
  </div>
</div>

<div id="modal-window-edit" class="modal hide fade" role="dialog" aria-labelledby="myModalLabelEdit" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content"></div>
  </div>
</div>

<div id="modal-window" class="modal hide fade" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content"></div>
  </div>
</div>